{
  "name": "@extropysk/payload-plugins",
  "version": "0.0.1",
  "private": true,
  "workspaces:": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo --filter \"./packages/**\" build",
    "dev": "pnpm --filter \"./packages/**\" run dev",
    "dev:cms": "pnpm --filter cms run dev",
    "lint": "eslint \"packages/**/*.ts\""
  },
  "devDependencies": {
    "@payloadcms/eslint-config": "^1.1.1",
    "@swc/cli": "0.1.65",
    "@swc/jest": "0.2.29",
    "@swc/register": "0.1.10",
    "@types/jest": "29.5.7",
    "@types/node": "20.5.7",
    "copyfiles": "2.4.1",
    "jest": "29.7.0",
    "nodemon": "3.0.3",
    "payload": "^2.30.4",
    "prettier": "^3.0.3",
    "ts-node": "10.9.2",
    "turbo": "^1.11.1",
    "typescript": "^4.8.4"
  },
  "engines": {
    "node": ">=14",
    "pnpm": ">=9.7.0"
  },
  "packageManager": "pnpm@9.7.0",
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier --write"
    ]
  }
}
